<?xml version="1.0" encoding="utf-8"?>
<LogConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <Enable>true</Enable>
    <TimerPeriod>100</TimerPeriod>
    <Writers>
        <File RootDirectory="Log111">
            <WriteType>ClownFish.Log.Serializer.FileWriter, ClownFish.Log</WriteType>
        </File>
        <Mail Receivers="liqf01@163.com.cn">
            <WriteType>ClownFish.Log.Serializer.MailWriter, ClownFish.Log</WriteType>
        </Mail>
        <MongoDb ConnectionString="mongodb://10.5.106.100/UnitTest?connectTimeout=5s;socketTimeout=5s">
            <WriteType>ClownFish.Log.Serializer.MongoDbWriter, ClownFish.Log</WriteType>
        </MongoDb>
        <Msmq RootPath=".\private$\ClownFish-Log-test">
            <WriteType>ClownFish.Log.Serializer.MsmqWriter, ClownFish.Log</WriteType>
        </Msmq>
        <WinLog LogName="ClownFish-Log" SourceName="WinLogWriter日志消息">
            <WriteType>ClownFish.Log.Serializer.WinLogWriter, ClownFish.Log</WriteType>
        </WinLog>
    </Writers>
    <Types>
		<!--如果要测试邮件功能，请先修改 App.config 配置邮件发送帐号，以及上面的Receivers属性表示的收件人-->
		<Type DataType="ClownFish.Log.Model.ExceptionInfo, ClownFish.Log" Writers="MongoDb,File,Msmq,WinLog" />
        <Type DataType="ClownFish.Log.Model.PerformanceInfo, ClownFish.Log" Writers="File,MongoDb" />
    </Types>
    <Performance DbExecuteTimeout="300" HttpExecuteTimeout="300" />
    <ExceptionWriter>WinLog</ExceptionWriter>
</LogConfig>