<ProxyMapRule>
    <Rules>
        <rule src="/new/api/test/add"           dest="{Demo.WebSiteApp.Url}/v20/api/WebSiteApp/test/Add.aspx" />
        <rule src="/new/api/test/ShowRequest"   dest="{Demo.WebSiteApp.Url}/v20/api/WebSiteApp/test/ShowRequest.aspx" />
        <rule src="/new/api/test/login"         dest="{Demo.WebSiteApp.Url}/v20/api/WebSiteApp/auth/login.aspx" />
        <rule src="/new/api/test/findByTel"     dest="{Demo.WebSiteApp.Url}/v20/api/WebSiteApp/Customer/findByTel.aspx" />
        
